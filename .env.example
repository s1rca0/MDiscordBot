# Discord Core
DISCORD_BOT_TOKEN=your-bot-token-here
COMMAND_PREFIX=$
OWNER_USER_ID=0
LOG_LEVEL=INFO
LOG_FILE=

# AI Provider
PROVIDER=groq
GROQ_API_KEY=your-groq-key
GROQ_MODEL_FAST=llama-3.1-8b-instant
GROQ_MODEL_SMART=llama-3.1-70b-versatile
AI_MODE_DEFAULT=smart
AI_TEMPERATURE=0.7
AI_MAX_NEW_TOKENS=512

# Invites
ENABLE_INVITES=true
SERVER_INVITE_URL=https://discord.gg/yourInvite

# Meme Feed (optional)
ENABLE_MEME_FEED=false
MEME_CHANNEL_ID=0
MEME_INTERVAL_MIN=120

# Disaster tools (optional)
ENABLE_DISASTER_TOOLS=false

# Tickets / Support (optional)
TICKET_HOME_CHANNEL_ID=0
SUPPORT_CHANNEL_ID=0

# YouTube announcements (optional)
YT_ANNOUNCE_CHANNEL_ID=0

# Safety
MAX_MESSAGE_LENGTH=1800